<?xml version = "1.0" encoding = "utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name = "relationClasses.StorageProduct" table = "inventory_product">

        <meta attribute = "class-description">
            This class contains the storage product detail.
        </meta>

        <id name = "id" type = "int" column="id">
            <generator class="native"/>
        </id>


        <property name = "storageId" column = "inventory_id" type = "int"/>
        <property name = "productId" column = "product_id" type = "int"/>

    </class>
</hibernate-mapping>